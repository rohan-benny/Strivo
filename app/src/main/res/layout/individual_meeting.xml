<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/meetingbck"
    android:layout_marginBottom="@dimen/_4sdp"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_64sdp">

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="@dimen/_32sdp"
        android:layout_height="@dimen/_32sdp"
        android:layout_marginStart="@dimen/_16sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mtlogo" />

    <TextView
        android:id="@+id/meetingdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="05 March 2025"
        android:fontFamily="@font/sorasemibold"
        android:textColor="@color/white"
        android:textSize="@dimen/_12ssp"
        android:layout_marginStart="@dimen/_14sdp"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/imageView13" />

    <TextView
        android:id="@+id/meetingdesc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Review Meeting"
        android:fontFamily="@font/soralight"
        android:textColor="@color/white"
        android:textSize="@dimen/_10ssp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView13"
        app:layout_constraintStart_toStartOf="@+id/meetingdate"
        app:layout_constraintTop_toBottomOf="@+id/meetingdate" />


</androidx.constraintlayout.widget.ConstraintLayout>