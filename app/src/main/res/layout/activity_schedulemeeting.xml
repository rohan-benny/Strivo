<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Schedulemeeting">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="@dimen/_14sdp"
        android:layout_height="@dimen/_14sdp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="@dimen/_46sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bckarrow" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_1sdp"
        android:fontFamily="@font/soraregular"
        android:text="Schedule Meeting"
        android:textColor="@color/black"
        android:textSize="@dimen/_15ssp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView20"
        app:layout_constraintStart_toEndOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_155sdp"
        android:layout_marginTop="@dimen/_16sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36"
        app:srcCompat="@drawable/schmtbg" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:fontFamily="@font/soraregular"
        android:text="Title"
        android:textColor="#19384D"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView21" />

    <EditText
        android:id="@+id/scmtitled"
        android:layout_width="@dimen/_270sdp"
        android:layout_height="@dimen/_35sdp"
        android:textSize="@dimen/_10ssp"
        android:background="@android:color/transparent"
        android:drawableBottom="@color/black"
        app:layout_constraintStart_toStartOf="@+id/textView37"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#19384D"
        app:layout_constraintBottom_toBottomOf="@+id/scmtitled"
        app:layout_constraintEnd_toEndOf="@+id/scmtitled"
        app:layout_constraintStart_toStartOf="@+id/scmtitled" />

    <TextView
        android:id="@+id/scmtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:fontFamily="@font/soraregular"
        android:text="Date"
        android:textColor="#19384D"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <EditText
        android:id="@+id/scmdate"
        android:layout_width="@dimen/_270sdp"
        android:layout_height="@dimen/_35sdp"
        android:textSize="@dimen/_10ssp"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/scmtitle"
        app:layout_constraintTop_toBottomOf="@+id/scmtitle" />

    <View
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#19384D"
        app:layout_constraintEnd_toEndOf="@+id/scmdate"
        app:layout_constraintStart_toStartOf="@+id/scmdate"
        app:layout_constraintTop_toBottomOf="@+id/scmdate" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:fontFamily="@font/soraregular"
        android:text="Time"
        android:textColor="#19384D"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintStart_toStartOf="@+id/scmdate"
        app:layout_constraintTop_toBottomOf="@+id/scmdate" />

    <EditText
        android:id="@+id/scmtime"
        android:layout_width="@dimen/_270sdp"
        android:layout_height="@dimen/_35sdp"
        android:textSize="@dimen/_10ssp"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="@+id/scmdate"
        app:layout_constraintStart_toStartOf="@+id/textView39"
        app:layout_constraintTop_toBottomOf="@+id/textView39" />

    <View
        android:id="@+id/view4"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#19384D"
        app:layout_constraintEnd_toEndOf="@+id/scmtime"
        app:layout_constraintStart_toStartOf="@+id/scmtime"
        app:layout_constraintTop_toBottomOf="@id/scmtime" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:fontFamily="@font/soraregular"
        android:text="Venue"
        android:textColor="#19384D"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintStart_toStartOf="@+id/scmtime"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <EditText
        android:id="@+id/scmvenue"
        android:layout_width="@dimen/_270sdp"
        android:layout_height="@dimen/_35sdp"
        android:textSize="@dimen/_10ssp"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="@+id/scmtime"
        app:layout_constraintStart_toStartOf="@+id/textView40"
        app:layout_constraintTop_toBottomOf="@+id/textView40" />

    <View
        android:id="@+id/view5"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#19384D"
        app:layout_constraintEnd_toEndOf="@+id/scmvenue"
        app:layout_constraintStart_toStartOf="@+id/scmvenue"
        app:layout_constraintTop_toBottomOf="@+id/scmvenue" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:fontFamily="@font/soraregular"
        android:text="Description"
        android:textColor="#19384D"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toBottomOf="@+id/scmvenue" />

    <EditText
        android:id="@+id/scmdesc"
        android:layout_width="0dp"
        android:layout_height="@dimen/_80sdp"
        android:layout_marginTop="5dp"
        android:background="@drawable/scmdesc"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:padding="5dp"
        android:textAlignment="textStart"
        android:textSize="@dimen/_10ssp"
        app:layout_constraintEnd_toEndOf="@+id/scmvenue"
        app:layout_constraintStart_toStartOf="@+id/textView41"
        app:layout_constraintTop_toBottomOf="@+id/textView41" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/schedulebtn"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_40sdp"
        android:layout_marginBottom="@dimen/_10sdp"
        android:background="@drawable/button"
        android:fontFamily="@font/sorasemibold"
        android:text="Schedule"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="@dimen/_13ssp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scmdesc" />

    <ImageView
        android:id="@+id/cal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="@+id/scmdate"
        app:layout_constraintTop_toTopOf="@+id/scmdate"
        app:srcCompat="@drawable/datepicker" />

</androidx.constraintlayout.widget.ConstraintLayout>